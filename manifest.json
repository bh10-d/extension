{
    "name": "Change Layout VKU Dao Tao Page",
    "version": "0.0.1",
    "description": "created by Bui Duc Hieu",
    "manifest_version": 2,
    "icons": {
        "16": "images/logo.png"
    },
    "browser_action": {
        "default_icon": "images/logo.png",
        "default_popup": "index.html",
        "default_title": "ghetsugiadoi"
    },
    "permissions": [
        "https://daotao.vku.udn.vn/sv/diem"
    ],
    "content_scripts": [
        {
            "matches": ["https://daotao.vku.udn.vn/sv/diem","https://daotao.vku.udn.vn/sv/khao-sat/cau-hoi-khao-sat/*"],
            "js": ["scripts/index.js"]
        }
    ]
}